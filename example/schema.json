{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Options",
    "description": "",
    "type": "object",
    "properties": {
        "resolve": {
            "type": "boolean",
            "description": "resolve refs"
        },
        "ref": {
            "type": "string",
            "description": "Reference to resolve or make root"
        },
        "language": {
            "enum": [
                "python",
                "js"
            ],
            "description": "choose lang"
        }
    },
    "additionalProperties": false,
    "required": [
        "resolve",
        "ref",
        "language"
    ]
}
