{
    "compilerOptions": {
      "target": "es5",
      "lib": [
        "dom",
        "dom.iterable",
        "esnext"
      ],
      "skipLibCheck": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strict": false,
      "forceConsistentCasingInFileNames": true,
      "module": "esnext",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "declaration": true,
      // "noEmit": true,
      "jsx": "preserve",
      "noUnusedLocals": false,
      "pretty": true
    },
    "include": [
      "src",
      "components", "components"
    ]
  }
  